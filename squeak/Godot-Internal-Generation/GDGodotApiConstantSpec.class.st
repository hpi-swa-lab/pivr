Class {
	#name : #GDGodotApiConstantSpec,
	#superclass : #GDMethodSpec,
	#instVars : [
		'value'
	],
	#category : #'Godot-Internal-Generation'
}

{ #category : #'instance creation' }
GDGodotApiConstantSpec class >> fromKey: aKeyString value: aValueObject className: className [

	^ (self newNamed: aKeyString arguments: {} category: self methodCategory className: className) value: aValueObject
]

{ #category : #'instance creation' }
GDGodotApiConstantSpec class >> methodCategory [

	^ '*autogenerated - constants'
]

{ #category : #'as yet unclassified' }
GDGodotApiConstantSpec class >> squeakifyMethodName: aString [

	^ super squeakifyMethodName: aString asLowercase
]

{ #category : #'as yet unclassified' }
GDGodotApiConstantSpec >> isClassMethod [

	^ true
]

{ #category : #generating }
GDGodotApiConstantSpec >> methodName [

	^ self class squeakifyMethodName: name
]

{ #category : #generating }
GDGodotApiConstantSpec >> source [

	^ '{1}
	<generated>

	^ {2}' format: {self methodName. value storeString}
]

{ #category : #accessing }
GDGodotApiConstantSpec >> value [

	^ value
]

{ #category : #accessing }
GDGodotApiConstantSpec >> value: anObject [

	value := anObject
]
