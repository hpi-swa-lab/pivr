Class {
	#name : #GDGodotApiPropertySetterSpec,
	#superclass : #GDMethodSpec,
	#instVars : [
		'setterName'
	],
	#category : #'Godot-Internal-Generation'
}

{ #category : #'instance creation' }
GDGodotApiPropertySetterSpec class >> fromAPIJsonObject: aJsonObject className: className [

	^ self
		newNamed: aJsonObject name
		arguments: {GDArgumentSpec fromApiPropertiesJsonObject: aJsonObject}
		category: self methodCategory
		className: className
]

{ #category : #'instance creation' }
GDGodotApiPropertySetterSpec class >> methodCategory [

	^ '*autogenerated - properties'
]

{ #category : #generating }
GDGodotApiPropertySetterSpec >> methodName [

	^ self class squeakifyMethodName: name
]

{ #category : #generating }
GDGodotApiPropertySetterSpec >> setterName [

	^ self class squeakifyMethodName: setterName
]

{ #category : #accessing }
GDGodotApiPropertySetterSpec >> setterName: aString [

	setterName := aString
]

{ #category : #generating }
GDGodotApiPropertySetterSpec >> source [

	^ '{1}{2}
	<generated>
		
	self set:{3} to: {4}' format: {
		self methodName.
		arguments first methodArgumentString.
		name asSymbol storeString.
		arguments first argumentParamterName}
]
