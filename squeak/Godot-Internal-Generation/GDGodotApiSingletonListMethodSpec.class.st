Class {
	#name : #GDGodotApiSingletonListMethodSpec,
	#superclass : #GDMethodSpec,
	#instVars : [
		'singletonNames'
	],
	#category : #'Godot-Internal-Generation'
}

{ #category : #constants }
GDGodotApiSingletonListMethodSpec class >> className [

	^ 'GDApi'
]

{ #category : #constants }
GDGodotApiSingletonListMethodSpec class >> methodCategory [

	^ '*autogenerated - constants'
]

{ #category : #constants }
GDGodotApiSingletonListMethodSpec class >> methodName [

	^ 'singletons'
]

{ #category : #'instance creation' }
GDGodotApiSingletonListMethodSpec class >> newFromClassSpecs: classSpecs [

	^ (self newNamed: self methodName
		arguments: #()
		category: self methodCategory
		className: self className)
		singletonNames: (classSpecs collect: #singletonName)
]

{ #category : #'as yet unclassified' }
GDGodotApiSingletonListMethodSpec >> isClassMethod [

	^ true
]

{ #category : #accessing }
GDGodotApiSingletonListMethodSpec >> singletonNames [

	^ singletonNames
]

{ #category : #accessing }
GDGodotApiSingletonListMethodSpec >> singletonNames: aStringCollection [

	singletonNames := aStringCollection
]

{ #category : #'as yet unclassified' }
GDGodotApiSingletonListMethodSpec >> singletonNamesString [

	^ (self singletonNames collect: [:name | String tab, String tab, '''', name, '''']) joinSeparatedBy: String cr
]

{ #category : #'as yet unclassified' }
GDGodotApiSingletonListMethodSpec >> source [

	^ '{1}
	<generated>
	
	^ #(
{2}
	)' format: {self name. self singletonNamesString}
]
